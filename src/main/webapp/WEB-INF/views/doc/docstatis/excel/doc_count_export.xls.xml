<?xml version="1.0"?>
<?mso-application progid="Excel.Sheet"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:o="urn:schemas-microsoft-com:office:office"
 xmlns:x="urn:schemas-microsoft-com:office:excel"
 xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:html="http://www.w3.org/TR/REC-html40">
 <Styles>
  <Style ss:ID="Default" ss:Name="Normal">
   <Alignment ss:Vertical="Bottom"/>
   <Borders/>
   <Font ss:FontName="宋体" x:CharSet="134" ss:Size="12"/>
   <Interior/>
   <NumberFormat/>
   <Protection/>
  </Style>
  <Style ss:ID="s64">
   <Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="宋体" x:CharSet="134"/>
  </Style>
  <Style ss:ID="s65">
   <Alignment ss:Horizontal="Left" ss:Vertical="Center" ss:WrapText="1"/>
   <Font ss:FontName="宋体" x:CharSet="134"/>
  </Style>
  <Style ss:ID="s66">
   <Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="宋体" x:CharSet="134" ss:Bold="1"/>
   <Interior ss:Color="#C0C0C0" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="s69">
   <Alignment ss:Horizontal="Center" ss:Vertical="Center"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="宋体" x:CharSet="134" ss:Size="12" ss:Bold="1"/>
  </Style>
 </Styles>
 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="7" ss:ExpandedRowCount="16" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="54" ss:DefaultRowHeight="14.25">
   <Column ss:AutoFitWidth="0" ss:Width="46.5"/>
   <Column ss:AutoFitWidth="0" ss:Width="332.25"/>
   <Column ss:AutoFitWidth="0" ss:Width="94.5"/>
   <Column ss:AutoFitWidth="0" ss:Width="26.5"/>
   <Column ss:AutoFitWidth="0" ss:Width="75.75" ss:Span="1"/>
   <Column ss:Index="7" ss:AutoFitWidth="0" ss:Width="95.25"/>
   <Row ss:AutoFitHeight="0" ss:Height="26.25">
    <Cell ss:MergeAcross="6" ss:StyleID="s69"><Data ss:Type="String">$!{showTitle}公文办理数量统计</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="21" ss:StyleID="s65">
    <Cell ss:StyleID="s66"><Data ss:Type="String">序号</Data></Cell>
    <Cell ss:StyleID="s66"><Data ss:Type="String">姓名</Data></Cell>
    <Cell ss:StyleID="s66"><Data ss:Type="String">创建文数</Data></Cell>
    <Cell ss:StyleID="s66"><Data ss:Type="String">经办</Data></Cell>
    <Cell ss:StyleID="s66"><Data ss:Type="String">待办</Data></Cell>
   </Row>
   #foreach($count in ${page})
   <Row ss:AutoFitHeight="0" ss:Height="21.75" ss:StyleID="s65">
    <Cell ss:StyleID="s64"><Data ss:Type="String">$velocityCount</Data></Cell>
    <Cell ss:StyleID="s64"><Data ss:Type="String">$!{count.userName}</Data></Cell>
    <Cell ss:StyleID="s64"><Data ss:Type="String">$!{count.createNum}</Data></Cell>
    <Cell ss:StyleID="s64"><Data ss:Type="String">$!{count.doBy}</Data></Cell>
    <Cell ss:StyleID="s64"><Data ss:Type="String">$!{count.doing}</Data></Cell>
   </Row>
   #end
  </Table>
 </Worksheet>
</Workbook>
