<?xml version="1.0"?>
<?mso-application progid="Excel.Sheet"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:o="urn:schemas-microsoft-com:office:office"
 xmlns:x="urn:schemas-microsoft-com:office:excel"
 xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:html="http://www.w3.org/TR/REC-html40">
 <Styles>
  <Style ss:ID="Default" ss:Name="Normal">
   <Alignment ss:Vertical="Bottom"/>
   <Borders/>
   <Font ss:FontName="宋体" x:CharSet="134" ss:Size="12"/>
   <Interior/>
   <NumberFormat/>
   <Protection/>
  </Style>
  <Style ss:ID="s62">
   <Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="宋体" x:CharSet="134"/>
  </Style>
  <Style ss:ID="s63">
   <Alignment ss:Horizontal="Left" ss:Vertical="Center" ss:WrapText="1"/>
   <Font ss:FontName="宋体" x:CharSet="134"/>
  </Style>
  <Style ss:ID="s64">
   <Font ss:FontName="Times New Roman" x:Family="Roman" ss:Size="12"
    ss:Underline="Single"/>
  </Style>
  <Style ss:ID="s65">
   <Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="宋体" x:CharSet="134" ss:Bold="1"/>
   <Interior ss:Color="#C0C0C0" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="s68">
   <Alignment ss:Horizontal="Center" ss:Vertical="Center"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="宋体" x:CharSet="134" ss:Size="12" ss:Bold="1"/>
  </Style>
 </Styles>
 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="8" ss:ExpandedRowCount="1000000000" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="54" ss:DefaultRowHeight="14.25">
   <Column ss:AutoFitWidth="0" ss:Width="46.5"/>
   <Column ss:AutoFitWidth="0" ss:Width="321"/>
   <Column ss:AutoFitWidth="0" ss:Width="188.25"/>
   <Column ss:AutoFitWidth="0" ss:Width="81"/>
   <Column ss:AutoFitWidth="0" ss:Width="96"/>
   <Column ss:AutoFitWidth="0" ss:Width="110"/>
   <Column ss:AutoFitWidth="0" ss:Width="110"/>
   <Column ss:AutoFitWidth="0" ss:Width="78.75"/>


   #if($processType == 'rec')
      <Row ss:AutoFitHeight="0" ss:Height="26.25">
        <Cell ss:MergeAcross="7" ss:StyleID="s68">
          <Data ss:Type="String">
            $!{title}
          </Data>
        </Cell>
      </Row>
      <Row ss:AutoFitHeight="0" ss:Height="21" ss:StyleID="s63">
        <Cell ss:StyleID="s65"><Data ss:Type="String">序号</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">文件标题</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">来文单位</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">编号</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">来文字号</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">来文日期</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">办理科室</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">状态</Data></Cell>
      </Row>
      #foreach($process in ${page.rows})
      <Row ss:AutoFitHeight="0" ss:Height="21.75" ss:StyleID="s63">
        <Cell ss:StyleID="s62"><Data ss:Type="String">$velocityCount</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.title}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.sendUnit}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.sncode}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.docCode}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.processShowDate}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.processDepName}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.status.value}</Data></Cell>
      </Row>
      #end
   #else
      <Row ss:AutoFitHeight="0" ss:Height="26.25">
        <Cell ss:MergeAcross="4" ss:StyleID="s68">
          <Data ss:Type="String">
            $!{title}
          </Data>
        </Cell>
      </Row>
      <Row ss:AutoFitHeight="0" ss:Height="21" ss:StyleID="s63">
        <Cell ss:StyleID="s65"><Data ss:Type="String">序号</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">文件标题</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">拟稿日期</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">拟稿部门</Data></Cell>
        <Cell ss:StyleID="s65"><Data ss:Type="String">状态</Data></Cell>
      </Row>
      #foreach($process in ${page.rows})
      <Row ss:AutoFitHeight="0" ss:Height="21.75" ss:StyleID="s63">
        <Cell ss:StyleID="s62"><Data ss:Type="String">$velocityCount</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.title}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.createDateShow}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.processDepName}</Data></Cell>
        <Cell ss:StyleID="s62"><Data ss:Type="String">$!{process.status.value}</Data></Cell>
      </Row>
      #end
    #end

  </Table>
 </Worksheet>
</Workbook>
