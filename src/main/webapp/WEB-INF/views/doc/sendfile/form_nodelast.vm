                <div data-options="region:'center'" >
                    #parse("doc/sendfile/inner/fwsign.vm")
                    </br>
                </div>
                <div data-options="region:'south'" class="mynoborder"  style="height:60px;">
                   <form id="editForm" method="post">
                     <input name="nextTaskKey" id="nextTaskKey" type="hidden" value=""/>
                     <input  id="flowId" name="flowId" type="hidden" value="$!{obj.flowId}"/>
                     <input  id="flowInstId" name="flowId" type="hidden" value="$!{flowInstId}"/>
                     <input  id="fordoId" name="fordoId" type="hidden" value="$!{fordoId}"/>
