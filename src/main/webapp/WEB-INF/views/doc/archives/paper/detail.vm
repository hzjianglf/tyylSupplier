<div class="easyui-panel mainborder-Padding" data-options="fit:true,border:false">
    <div id="cc" class="easyui-layout" data-options="fit:true">
        <div data-options="region:'center'" class="mynoborder">
            <form id="editForm" method="post">
				<input name="fileId" id="fileId" type="hidden" value="$!{obj.id}"/>
				<input name="title" id="title" type="hidden" value="$!{obj.title}"/>
                <table class="table-edit" width="100%" border="0" cellspacing="1" cellpadding="0">
                    <tbody>
                        <tr>
                            <td class="fix_td_title">&nbsp;</td>
			                <td class="fix_td_data">&nbsp;</td>
			                <td class="fix_td_title">&nbsp;</td>
			                <td class="fix_td_data">&nbsp;</td>
                        </tr>
                        <tr align="center">
                            <td class="bg-grey"><span><font color=red>*</font></span>文件标题</td>
                            <td class="bg-white" colspan="3">
								$!{esc.html($!{obj.title})}
                            </td>
                        </tr>
						<tr align="center">
							<td class="bg-grey">文号</td>
							<td class="bg-white">
								$!{esc.html($!{obj.docCode})}
                            </td>
							<td class="bg-grey">紧急程度</td>
							<td class="bg-white">
								$!{esc.html($!{obj.urgencyLevel})}
                            </td>
                        </tr>
						<tr align="center">
							<td class="bg-grey">编号</td>
							<td class="bg-white">
								$!{esc.html($!{obj.sncode})}
                            </td>
							<td class="bg-grey">密级</td>
							<td class="bg-white">
								$!{esc.html($!{obj.dense})}
                            </td>
                        </tr>
						<tr align="center">
							<td class="bg-grey">拟稿人</td>
							<td class="bg-white">
								$!{esc.html($!{obj.creatorName})}
                            </td>
							<td class="bg-grey">拟稿时间</td>
							<td class="bg-white">
								$!{date.format('yyyy-MM-dd',$!{archives.createDate})}
                            </td>
                        </tr>
						<tr align="center">
							<td class="bg-grey">附件</td>
							<td colSpan="3" class="bg-white" attachId="$!{obj.id}">
								#formfilemulty("attach", "", "", "", $!{attachMap.get("$obj.id")},"#defaultDownloadUrl('docPaperAttach')", false, "#defaultUploadUrl('docPaperAttach')",false)
							</td>
                        </tr>
						
						<tr align="center">
							<td class="bg-grey" style="text-align: center;"  colspan="4" >档案信息</td>
                        </tr>
						
						
						<tr align="center">
                            <td class="bg-grey" align="right" width="120px">柜位号</td>
                            <td  align="left" class="bg-white">
                                $!{esc.html($!{archives.counterNoName})}
                            </td>
							<td class="bg-grey" align="right" width="120px">案卷类型</td>
                            <td  align="left" class="bg-white">
								$!{esc.html($!{archives.archTypeName})}
                            </td>
                        </tr>
						<tr align="center">
							<td class="bg-grey" align="right" width="120px">案卷号</td>
                            <td  align="left" class="bg-white">
								$!{esc.html($!{archives.archNo})}
                            </td>
                            <td class="bg-grey" align="right" width="120px">案卷名</td>
                            <td  align="left" class="bg-white">
								$!{esc.html($!{archives.archName})}
                            </td>
                        </tr>
						<tr align="center">
                            <td class="bg-grey" align="right" width="120px">介质类型</td>
                            <td  align="left" class="bg-white">
                                $!{esc.html($!{archives.mediumTypeName})}
                            </td>
							<td class="bg-grey" align="right">年份</td>
                            <td align="left" class="bg-white">
								$!{esc.html($!{archives.archYear})}
                            </td>
                        </tr>
						<tr align="center">
							<td class="bg-grey" align="right">保留日期</td>
							<td align="left" class="bg-white">
								$!{date.format('yyyy-MM-dd',$!{archives.retentionDate})}
							</td>
							<td class="bg-grey" align="right">立卷日期</td>
							<td align="left" class="bg-white">
								$!{date.format('yyyy-MM-dd',$!{archives.createDate})}
							</td>
                        </tr>
						<tr align="center">
                            <td class="bg-grey" align="right">排序号</td>
                            <td align="left" class="bg-white" colspan="3">
								$!{esc.html($!{archives.orderNum})}
                            </td>
                        </tr>
                        <tr align="center">
                          <td class="bg-grey" align="right">备注</td>
                          <td align="left" class="bg-white" colspan="3">
                            <textarea name="remark" class="easyui-validatebox textareaborder" data-options="validType:'maxLength[250]'" readonly style="width:95%;height:80px;resize:none;" >$!{esc.html($!{archives.remark})}</textarea>
                          </td>
                        </tr>
                    </tbody>
                </table>
            </form>
        </div>
        <div data-options="region:'south'" style="height:52px;padding-top:15px; background-color: #F4F3F3;">
            <div class="bottom-but-dialog">
                <ul align="right">
                    <li><a id="back" onclick="top.CC.closeCurTab()" class="new-but">关闭</a></li>
                </ul>
            </div>
        </div>
    </div>
</div>
#@script()
<script type="text/javascript">

    var _baseUrl = '$link.getContextURL()';

    //返回
    function back() {
        $('#editWindow').window('close');
    }
	
</script>
#end
